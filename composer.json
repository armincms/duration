{
    "name": "armincms/duration",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": { 
        "php": ">=8.0",
        "illuminate/support": "~9.0" 
    },
    "require-dev": {
        "laravel/pint": "^1.2" 
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Duration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Duration\\DurationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}
